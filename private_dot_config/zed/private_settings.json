{
  "audio": {
    "experimental.auto_microphone_volume": true,
    "experimental.rodio_audio": true
  },
  "document_folding_ranges": "on",
  "semantic_tokens": "combined",
  "vim": {
    "use_smartcase_find": true,
    "gdefault": true,
    "toggle_relative_line_numbers": true
  },
  "context_servers": {
    "mise": {
      "enabled": true,
      "remote": false,
      "command": "mise",
      "args": [
        "mcp"
      ],
      "env": {
        "MISE_EXPERIMENTAL": "1"
      }
    },
    "time": {
      "enabled": true,
      "remote": false,
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "dhi.io/time-mcp:2025"
      ]
    },
    "filesystem": {
      "env": {},
      "enabled": true,
      "remote": false,
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "-v",
        ".:/workspace:rw",
        "dhi.io/filesystem-mcp:2025",
        "/workspace"
      ]
    },
    "memory": {
      "env": {},
      "enabled": true,
      "remote": false,
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "dhi.io/memory-mcp:2025"
      ]
    },
    "fetch": {
      "enabled": true,
      "remote": false,
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "dhi.io/fetch-mcp:2025"
      ]
    },
    "git": {
      "env": {},
      "enabled": true,
      "remote": false,
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "-v",
        ".:/repo:rw",
        "-v",
        "/Users/karl/.ssh:/home/nonroot/.ssh:ro",
        "-v",
        "/Users/karl/.config/git/config:/home/nonroot/.gitconfig:ro",
        "dhi.io/git-mcp:2025",
        "--repository",
        "/repo"
      ]
    },
    "docker-hub": {
      "env": {},
      "enabled": true,
      "remote": false,
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "dhi.io/hub-mcp:0"
      ]
    },
    "github": {
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      },
      "enabled": true,
      "remote": false,
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "dhi.io/github-mcp:0"
      ]
    },
    "chrome-devtools-mcp-zed": {
      "enabled": true,
      "remote": false,
      "settings": {
        "browser_url": null,
        "ws_endpoint": null,
        "ws_headers": null,
        "auto_connect": null,
        "channel": null,
        "headless": null,
        "isolated": null,
        "user_data_dir": null,
        "viewport": null,
        "executable_path": null,
        "chrome_arg": [],
        "proxy_server": null,
        "accept_insecure_certs": null,
        "category_emulation": null,
        "category_performance": null,
        "category_network": null,
        "log_file": null,
        "extra_args": []
      }
    }
  },
  "agent_servers": {
    "codex": {
      "default_config_options": {
        "mode": "full-access",
        "model": "gpt-5.3-codex",
        "reasoning_effort": "high"
      },
      "default_mode": "auto",
      "default_model": "gpt-5.3-codex/high"
    }
  },
  "bottom_dock_layout": "contained",
  "status_bar": {
    "active_encoding_button": "non_utf8"
  },
  "which_key": {
    "enabled": true
  },
  "buffer_line_height": "comfortable",
  "ui_font_family": "Monaspace Neon",
  "ssh_connections": [
    {
      "host": "orb",
      "args": [],
      "projects": []
    }
  ],
  "centered_layout": {
    "right_padding": null,
    "left_padding": null
  },
  "completion_menu_scrollbar": "auto",
  "restore_on_startup": "launchpad",
  "indent_guides": {
    "background_coloring": "disabled",
    "coloring": "indent_aware"
  },
  "minimap": {
    "show": "auto"
  },
  "colorize_brackets": true,
  "buffer_font_family": "Monaspace Neon",
  "git_panel": {
    "tree_view": true,
    "status_style": "icon",
    "sort_by_path": true,
    "collapse_untracked_diff": true,
  },
  "project_panel": {
    "hide_hidden": false,
    "bold_folder_labels": true,
    "sort_mode": "directories_first",
    "show_diagnostics": "errors",
    "git_status": true,
    "hide_gitignore": false,
    "default_width": 240.0,
    "entry_spacing": "comfortable",
    "hide_root": true,
  },
  "search": {
    "include_ignored": true,
    "regex": true,
    "center_on_match": true,
  },
  "use_smartcase_search": true,
  "sticky_scroll": {
    "enabled": true,
  },
  "autoscroll_on_clicks": true,
  "show_signature_help_after_edits": true,
  "auto_signature_help": true,
  "relative_line_numbers": "enabled",
  "toolbar": {
    "code_actions": true,
  },
  "always_treat_brackets_as_autoclosed": true,
  "inlay_hints": {
    "show_background": true,
    "enabled": true,
  },
  "edit_predictions": {
    "mode": "eager",
  },
  "agent": {
    "show_turn_stats": true,
    "tool_permissions": {
      "default": "allow"
    },
    "inline_assistant_model": {
      "provider": "copilot_chat",
      "model": "claude-haiku-4.5",
    },
    "use_modifier_to_send": true,
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-5-mini",
    },
    "play_sound_when_agent_done": true,
    "default_profile": "ask",
    "model_parameters": [],
  },
  "diagnostics": {
    "inline": {
      "enabled": true,
    },
  },
  "prettier": {
    "allowed": false,
  },
  "close_on_file_delete": true,
  "use_system_window_tabs": true,
  "preview_tabs": {
    "enable_preview_multibuffer_from_code_navigation": true,
    "enable_keep_preview_on_code_navigation": true,
    "enable_preview_from_file_finder": true,
  },
  "tabs": {
    "show_diagnostics": "errors",
    "file_icons": true,
    "git_status": true,
  },
  "title_bar": {
    "show_menus": true,
    "show_branch_icon": true,
  },
  "terminal": {
    "shell": "system",
    "toolbar": {
      "breadcrumbs": true,
    },
    "option_as_meta": true,
    "copy_on_select": true,
  },
  "git": {
    "inline_blame": {
      "show_commit_summary": true,
    },
  },
  "calls": {
    "mute_on_join": true,
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000,
    },
  },
  "helix_mode": true,
  "icon_theme": {
    "mode": "system",
    "light": "Catppuccin Latte",
    "dark": "Catppuccin Mocha",
  },
  "redact_private_values": true,
  "vim_mode": true,
  "ui_font_size": 14.0,
  "buffer_font_size": 14.0,
  "theme": {
    "mode": "system",
    "light": "Catppuccin Latte",
    "dark": "Catppuccin Mocha",
  },
}
